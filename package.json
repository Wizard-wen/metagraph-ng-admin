{
  "name": "metagraph-ng-manager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port $port --proxy-config $proxyConfig",
    "dev": "ng serve --host 0.0.0.0 --disableHostCheck --port 34089 --proxy-config proxy.conf.json",
    "build": "ng build --prod --aot --output-hashing all --base-href ./ ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "metagraph-constant": "0.0.39",
    "ng-json-view": "^4.0.0",
    "ng-zorro-antd": "9.3.0",
    "qiniu-js": "^3.1.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-eslint/builder": "^0.3.0-beta.1",
    "@angular-eslint/eslint-plugin": "^0.3.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "^0.3.0-beta.1",
    "@angular-eslint/schematics": "^0.3.0-beta.1",
    "@angular-eslint/template-parser": "^0.3.0-beta.1",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "codelyzer": "^6.0.0-next.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
